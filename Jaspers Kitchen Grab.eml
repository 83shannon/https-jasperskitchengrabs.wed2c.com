@charset "utf-8";

.header-com { position: relative; display: flex; align-items: center; justify-content: center; height: 0.44rem; background-color: rgb(68, 0, 250); }

.header-com.transparent { background-color: inherit; }

.header-com.transparent .iconfont, .header-com.transparent .title { color: rgb(48, 49, 51); }

.header-com.gray { background-color: rgb(240, 242, 245); }

.header-com.gray .iconfont, .header-com.gray .title { color: rgb(48, 49, 51); }

.header-com.white { background-color: rgb(255, 255, 255); }

.header-com.white .iconfont, .header-com.white .title { color: rgb(48, 49, 51); }

.header-com.violet { background-color: rgb(116, 64, 255); }

.header-com.violet .iconfont, .header-com.violet .title { color: rgb(254, 254, 254); }

.header-com .iconfont { position: absolute; left: 0.15rem; font-size: 20px; color: rgb(255, 255, 255); }

.header-com .title { font-size: 16px; line-height: 24px; font-weight: 500; color: rgb(255, 255, 255); display: -webkit-box; overflow: hidden; -webkit-line-clamp: 1; -webkit-box-orient: vertical; word-break: break-all; }

.maiThemePagecom { position: relative; width: 100%; }

.maiThemePagecom .main-theme-header { position: sticky; top: 0px; width: 100%; max-width: 5.5rem; z-index: 3; }

.maiThemePagecom .main-theme-div { position: fixed; left: 50%; width: 100%; max-width: 5.5rem; height: 0.56rem; background-color: rgb(68, 0, 250); z-index: 0; transform: translate(-50%); }

.maiThemePagecom .main-theme-div + .mainThemeMain { position: relative; margin: 0.21rem 0.15rem 0px; }

.maiThemePagecom .main-theme-div + .mainThemeMainNoHeader { background-color: red; }

.maiThemePagecom.noHeader { padding-top: 0px; }

.maiThemePagecom.noHeader .main-theme-div { position: fixed; left: 50%; width: 100%; max-width: 5.5rem; height: 0px; background-color: rgb(68, 0, 250); z-index: 0; transform: translate(-50%); }

.maiThemePagecom.noHeader .main-theme-div + .mainThemeMain { position: relative; margin: 0px 0.15rem; }

.maiThemePagecom.noHeader .main-theme-div + .mainThemeMainNoHeader { margin: 0.3rem; }

.maiThemePagecom .mainThemeFixedBottom { position: fixed; bottom: 0px; left: 0px; right: 0px; margin: 0px auto; width: 100%; max-width: 5.5rem; background: rgb(240, 242, 245); z-index: 1; }

.maiThemePagecom .mainThemeFixedBottom::after { content: ""; position: absolute; left: 0px; top: -0.15rem; width: 100%; height: 0.15rem; background: linear-gradient(rgba(240, 242, 245, 0), rgb(240, 242, 245)); }

.maiThemePagecom .mainThemeFixedBottom.noFixed { margin-top: 0.16rem; display: block; position: relative; width: 100%; max-width: 5.5rem; background: rgb(240, 242, 245); z-index: 1; }

#hide-for-jump-top { position: absolute; top: -0.8rem; }

.am-list-header { padding: 0.15rem 0.15rem 0.09rem; font-size: 14px; color: rgb(136, 136, 136); width: 100%; box-sizing: border-box; }

.am-list-footer { padding: 0.09rem 0.15rem 0.15rem; font-size: 14px; color: rgb(136, 136, 136); }

.am-list-body { position: relative; background-color: rgb(255, 255, 255); border-top: 1px solid rgb(221, 221, 221); border-bottom: 1px solid rgb(221, 221, 221); }

@media (-webkit-min-device-pixel-ratio: 2), (min-resolution: 2dppx) {
  html:not([data-scale]) .am-list-body { border-top: none; }
  html:not([data-scale]) .am-list-body::before { content: ""; position: absolute; background-color: rgb(221, 221, 221); display: block; z-index: 1; inset: 0px auto auto 0px; width: 100%; height: 1px; transform-origin: 50% 50%; transform: scaleY(0.5); }
}

@media (-webkit-min-device-pixel-ratio: 2) and (-webkit-min-device-pixel-ratio: 3), (min-resolution: 2dppx) and (min-resolution: 3dppx) {
  html:not([data-scale]) .am-list-body::before { transform: scaleY(0.33); }
}

